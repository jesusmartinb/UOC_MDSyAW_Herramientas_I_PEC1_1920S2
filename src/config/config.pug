-const titles = ['Desarrollo Web - Como construir una web', 'Desarrollo Web - Aprende habilidades básicas', 'Desarrollo Web - Programa sitios web']
-let menus = ['Inicio', 'HTML', 'CSS', 'JavaScript', 'Herramientas']
-let hrefes = ['index.html', 'html.html', 'css.html', 'javascript.html', 'herramientas.html']
mixin caja(fa,titulo,contenido)
    .caja
        .caja__fa: i(class=fa)
        .caja__contenido
            h3= titulo
            p= contenido
            a.boton Leer más
mixin enlace(hrefes,menus)
    a(href=`${hrefes}`)= menus